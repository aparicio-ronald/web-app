<%= form_with(model: professor, local: true) do |form| %>
  <% if professor.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(professor.errors.count, "error") %> prohibited this professor from being saved:</h2>

      <ul>
      <% professor.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :professor_name %>
  </div>

  <div class="field">
    <%= form.label :course_name %>
    <%= form.text_field :course_name, id: :professor_course_name %>
  </div>

  <div class="field">
    <%= form.label :section_number %>
    <%= form.number_field :section_number, id: :professor_section_number %>
  </div>

  <div class="field">
    <%= form.label :student_id %>
    <%= form.collection_select :student_id, @students, :id, :id_number%>
  </div>

     <div class="field">
    <%= form.label :section_id %>
    <%= form.collection_select :section_id, @sections, :id, :section_number%>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
